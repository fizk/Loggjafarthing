{
  "name": "fizk/loggjafarthing",
  "description": "API for the Loggjafarthing system",
  "type": "project",
  "license": "BSD-3-Clause",
  "keywords": [
    "framework",
    "mvc",
    "zf"
  ],
  "homepage": "https://github.com/fizk/Loggjafarthing",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/fizk/Rend"
    },
    {
      "type": "vcs",
      "url": "https://github.com/fizk/dbunit"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.4",
    "ext-pdo": "*",
    "ext-json": "*",
    "ext-dom": "*",
    "ext-mongodb": "*",
    "elasticsearch/elasticsearch": "^7.9",
    "fizk/rend": "6.0.0",
    "laminas/laminas-mvc": "^3.1",
    "laminas/laminas-cache": "^2.9",
    "laminas/laminas-form": "^2.15",
    "league/html-to-markdown": "^4.10",
    "mongodb/mongodb": "^1.7",
    "monolog/monolog": "^2.1",
    "php-amqplib/php-amqplib": "^2.12",
    "zeptech/annotations": "^1.1"

  },
  "require-dev": {
    "phpunit/phpunit": "^9.4",
    "mockery/mockery": "^1.4",
    "squizlabs/php_codesniffer": "^3.5",
    "fizk/dbunit": "^5.0",
    "laminas/laminas-test": "^3.4",
    "zumba/mongounit": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Althingi\\": "module/Althingi/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AlthingiTest\\": "module/Althingi/tests/"
    }
  }
}
