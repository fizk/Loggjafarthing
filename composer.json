{
  "name": "fizk/althingi-source",
  "description": "API and main data-store for the Loggjafarthing system",
  "type": "project",
  "license": "BSD-3-Clause",
  "keywords": [],
  "homepage": "https://github.com/fizk/althingi-source",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/fizk/dbunit"
    },
    {
      "type": "vcs",
      "url": "https://github.com/fizk/laminas-cache"
    },
    {
      "type": "vcs",
      "url": "https://github.com/fizk/laminas-cache-storage-adapter-redis"
    },
    {
        "type": "git",
        "url": "https://github.com/dantleech/phly-event-dispatcher.git"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8",
    "ext-pdo": "*",
    "ext-json": "*",
    "ext-dom": "*",
    "fizk/laminas-cache": "3.0.5",
    "fizk/laminas-cache-storage-adapter-redis": "^2.0.1",
    "laminas/laminas-cache-storage-adapter-blackhole": "^1.0",
    "laminas/laminas-cache-storage-adapter-filesystem": "^1.0",
    "laminas/laminas-form": "^2.17",
    "league/html-to-markdown": "^5.0",
    "monolog/monolog": "^2.2",
    "phly/phly-event-dispatcher": "dev-php8",
    "php-amqplib/php-amqplib": "^3.0",
    "zeptech/annotations": "^1.1",
    "laminas/laminas-diactoros": "^2.5",
    "laminas/laminas-httphandlerrunner": "^1.4"

  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "mockery/mockery": "^1.4",
    "squizlabs/php_codesniffer": "^3.6",
    "fizk/dbunit": "^5.1"
  },
  "autoload": {
    "psr-4": {
      "Althingi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AlthingiTest\\": "tests/"
    }
  }
}
