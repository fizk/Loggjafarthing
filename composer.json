{
  "name": "fizk/althingi-source",
  "description": "API for the Loggjafarthing system",
  "type": "project",
  "license": "BSD-3-Clause",
  "keywords": [],
  "homepage": "https://github.com/fizk/althingi-source",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/fizk/dbunit"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.4",
    "ext-pdo": "*",
    "ext-json": "*",
    "ext-dom": "*",
    "laminas/laminas-cache": "^2.9",
    "laminas/laminas-form": "^2.15",
    "league/html-to-markdown": "^4.10",
    "monolog/monolog": "^2.1",
    "php-amqplib/php-amqplib": "^2.12",
    "zeptech/annotations": "^1.1",
    "laminas/laminas-diactoros": "^2.4",
    "laminas/laminas-httphandlerrunner": "^1.2"

  },
  "require-dev": {
    "phpunit/phpunit": "^9.4",
    "mockery/mockery": "^1.4",
    "squizlabs/php_codesniffer": "^3.5",
    "fizk/dbunit": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "Althingi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AlthingiTest\\": "tests/"
    }
  }
}
