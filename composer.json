{
  "name": "fizk/loggjafarthing",
  "description": "API for the Loggjafarthing system",
  "type": "project",
  "license": "BSD-3-Clause",
  "keywords": [
    "framework",
    "mvc",
    "zf"
  ],
  "homepage": "https://github.com/fizk/Loggjafarthing",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/fizk/Rend"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.3",
    "ext-pdo": "*",
    "ext-json": "*",
    "ext-dom": "*",
    "ext-mongodb": "*",
    "zendframework/zend-mvc": "^3.0.1",
    "zendframework/zend-mvc-console": "^1.2",
    "zendframework/zend-eventmanager": "^3.2",
    "zendframework/zend-cache": "^2.8",
    "zendframework/zend-json": "^3.1",
    "zendframework/zend-hydrator": "^2.4",
    "fizk/rend": "5.2.1",
    "league/html-to-markdown": "^4.7",
    "elasticsearch/elasticsearch": "^6.0",
    "zeptech/annotations": "^1.1",
    "monolog/monolog": "^1.23",
    "php-amqplib/php-amqplib": "^2.8",
    "mongodb/mongodb": "1.5.1",
    "zendframework/zend-progressbar": "^2.6"
  },
  "autoload": {
    "psr-4": {
      "Althingi\\": "module/Althingi/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AlthingiTest\\": "module/Althingi/tests/"
    }
  },
  "extra": [],
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "development-disable": "zf-development-mode disable",
    "development-enable": "zf-development-mode enable",
    "development-status": "zf-development-mode status",
    "post-create-project-cmd": [
      "@development-enable"
    ],
    "serve": "php -S 0.0.0.0:8080 -t public public/index.php",
    "test": "phpunit"
  },
  "require-dev": {
    "zendframework/zend-test": "^3.2",
    "phpunit/phpunit": "^7",
    "mockery/mockery": "^1.1",
    "squizlabs/php_codesniffer": "^3.3",
    "phpunit/dbunit": "^4.0",
    "phpmd/phpmd": "^2.6",
    "zumba/mongounit": "^4.0",
    "guzzlehttp/ringphp": "^1.1"
  }
}
